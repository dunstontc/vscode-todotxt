{
  "name": "vscode-todotxt",
  "displayName": "vscode-todotxt",
  "description": "Just syntax highlighting. (WIP)",
  "version": "1.0.2",
  "publisher": "dunstontc",
  "repository": {
    "type": "git",
    "url": "https://github.com/dunstontc/vscode-todotxt.git"
  },
  "engines": { "vscode": "^1.21.0" },
  "categories": ["Programming Languages"],
  "galleryBanner": {
    "color": "#608b4e",
    "theme": "dark"
  },
  "icon": "assets/logo.png",
  "scripts": {
    "build": "json5 ./src/todo.tmLanguage.json5 -o ./syntaxes/todo.tmLanguage.json -s 2;"
  },
  "contributes": {
    "languages": [{
      "id": "todo",
      "aliases": [
        "todo",
        "todo"
      ],
      "extensions": [
        "todo.txt",
        "done.txt"
      ],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "todo",
      "scopeName": "source.todo",
      "path": "./syntaxes/todo.tmLanguage.json"
    }]
  }
}
